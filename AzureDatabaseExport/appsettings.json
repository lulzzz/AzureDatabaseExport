{
  "Threading": {
    "BoundedCapacity": 100,
    "MaxDegreeOfParallelism": 1
  },
  "Backups": [
    {
      "Source": {
        "SqlServerResourceGroup": "<SQL-SERVER-RESOURCE-GROUP>",
        "SqlServerName": "<SQL-SERVER-NAME>",
        "SqlDatabaseName": "<SQL-DATABASE-NAME>",
        "SqlAdminUsername": "<SQL-ADMIN-USERNAME>",
        "SqlAdminPassword": "<SQL-ADMIN-PASSWORD>"
      },
      "Destination": {
        "StorageAccountResourceGroup": "<STORAGE-ACCOUNT-RESOURCE-GROUP>",
        "StorageAccountName": "<STORAGE-ACCOUNT-NAME>",
        "StorageContainerName": "<STORAGE-CONTAINER-NAME>",
        "StorageContainerConnectionString": "<STORAGE-CONTAINER-CONNECTION-STRING>",
        "DeleteFromStorageAfterDownload": true,
        "LocalDirectory": "<LOCAL-DIRECTORY-PATH>"
      }
    }
  ]
}
